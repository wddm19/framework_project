<section class="grid-box">
    {{#each student}}
    <div class="profile-smallimg">
        <div class="profile__image"><img src="{{this.photo}}"></div>
        <div class="profile__info">
            <h3>{{this.name}}</h3>
            <p class="profile__info__extra"> {{this.program}}</p>
            <p>{{this.intro}}</p>
        </div>
        <div class="profile__stats">
            <p class="profile__stats__title">{{this.skill}}</p>
        </div>
        <div class="profile__cta">
            <a href="{{this.detailsURL}}" class="button">See More</a>
        </div>
    </div>
    {{/each}}
</section>